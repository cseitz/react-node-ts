{
    "compilerOptions": {
        "esModuleInterop": true,
        "jsx": "react",
        "module": "commonjs",
        "moduleResolution": "node",
        "lib": [
            "dom",
            "esnext"
        ],
        "strict": false,
        "target": "es6",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "outDir": "build/server",
        "baseUrl": ".",
        "rootDir": "./src",
        "paths": {
            "@lib*": ["./src/lib*", "./src/lib*.js"]
        }
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules",
        "build"
    ]
}